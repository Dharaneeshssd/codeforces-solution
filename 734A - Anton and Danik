#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    string s;
    cin>>n;
    cin>>s;
    int d=count(s.begin(),s.end(),'D');
    int a=count(s.begin(),s.end(),'A');
    if (a==d)
    cout<<"Friendship";
    else if (d>a)
    cout<<"Danik";
    else
    cout<<"Anton";
    return 0;
}
